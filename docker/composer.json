{
    "name": "tests/project01",
    "description": "checks the page for data in the db",
    "type": "project",
    "require": {
        "php": ">=8.3",
        "ext-mysqli": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "chrome-php/chrome": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "scripts": {
        "test": "phpunit --configuration phpunit.xml"
    }
}
